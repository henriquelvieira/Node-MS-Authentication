{
  "name": "ms-authentication",
  "version": "1.0.0",
  "description": "Microservice criado em Node.js e Typescript",
  "main": "./dist/server.js",
  "scripts": {
    "test": "jest",
    "test:functional": "jest --projects ./src/routes --runInBand",
    "start": "node ./",
    "build": "tsc -p .",
    "deploy": "tsc -p . && node ./",
    "test:start": "jest && node ./",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/henriquelvieira/Node-MS-Authentication.git"
  },
  "homepage": "https://github.com/henriquelvieira/Node-MS-Authentication#readme",
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "engines": {
    "node": "16"
  },
  "author": "Henrique Lopes Vieira",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.7",
    "@types/pg": "^8.6.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "ts-dotenv": "^0.8.3",
    "uuid": "^8.3.2"
  }
}
